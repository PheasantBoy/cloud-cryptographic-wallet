{
  "name": "aws-kms-ethers-signer",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/odanado/aws-kms-provider"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./dist/index.js",
      "node": "./dist/index.js",
      "default": "./lib/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "lib",
    "package.json",
    "yarn.lock"
